{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit --skipLibCheck",
    "generate:types": "swagger-typescript-api generate -p ../backend/docs/api/swagger.json -o ./src/types -n api.generated.ts --modular",
    "generate:types:remote": "swagger-typescript-api generate -p http://localhost:8080/swagger/doc.json -o ./src/types -n api.generated.ts --modular",
    "generate:api": "swagger-typescript-api generate -p ../backend/docs/api/swagger.json -o ./src/lib/api/generated -n api.ts --modular --axios",
    "generate:all": "yarn generate:types"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-form": "^0.1.8",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.548.0",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.86.8",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "openapi-typescript": "^7.10.1",
    "swagger-typescript-api": "^13.2.16",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
